/********************************
*       Shear1D                 *
********************************/

:SharedLibs=Shear1D
{
   Shear1D    = $SharedLibDirEOS/ExtPlastic1D
   ConstGV    = $SharedLibDirEOS/ExtPlastic1D
   PolyG      = $SharedLibDirEOS/ExtPlastic1D
}
.base  = Shear1D
.units = hydro::std


Shear1D:ConstGV=HMX; units=hydro::std
{
    GV      = 12./1.9
}

Shear1D:PolyG=PMMA; units=hydro::std
{
     V0  = 1./1.185
    G(1) = 2.31, 15, 80
}


/********************************
*       Elastic1D               *
********************************/

:SharedLibs.=EOS
{
    Elastic1D      = $SharedLibDirEOS/ExtPlastic1D
    ElasticConstGV = $SharedLibDirEOS/ExtPlastic1D
    ElasticPolyG   = $SharedLibDirEOS/ExtPlastic1D
}
.base  = EOS
.units = hydro::std


EOS:Elastic1D=HMX; units=hydro::std
{
   EOS     =   "Hayes::HMX"
   Shear1D = "ConstGV::HMX"
}

EOS:ElasticConstGV=HMX
{
   EOS  = "Hayes::HMX"
   Gref = 12
}

EOS:ElasticPolyG=PMMA; units=hydro::std
{
   EOS   = "PeSpline::PMMA-hydro"
   PolyG = "PMMA"
}
