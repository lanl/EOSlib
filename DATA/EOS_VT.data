/********************************
*       EOS_VT                  *
********************************/

:SharedLibs=EOS_VT
{
    EOS_VT         = $SharedLibDirEOS/EOS
    IdealGas       = $SharedLibDirEOS/libMaterials
    StiffenedGas   = $SharedLibDirEOS/libMaterials
    Hayes          = $SharedLibDirEOS/libMaterials
    HayesBM        = $SharedLibDirEOS/libMaterials
    BirchMurnaghan = $SharedLibDirEOS/libMaterials
    Keane          = $SharedLibDirEOS/libMaterials
    UsUp           = $SharedLibDirEOS/libMaterials
    UsUpSpline     = $SharedLibDirEOS/libMaterials
    PeSpline       = $SharedLibDirEOS/libMaterials
    JWL            = $SharedLibDirEOS/libMaterials
}
.base  = EOS_VT
.units = hydro::std


EOS_VT:Hayes=HMX; units=hydro::std
{
    T_ref = 300
    rho0 = 1.9      # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    K0 = 12.9       # GPa
    K0 = 13.5       # Olingers hydrostatic data
    N  =  9.3
    Cv = 1.5e-3     # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    #c0=2.6778 s = 2.68946
}

EOS_VT:IdealGas=5/3; units=hydro::std
{
    gamma = 5/3
    Cv    = 1.5e-3
    V_ref = 3000 
    T_ref = 300
}
