HEfit=PBX9501; units=hydro::std
{
# Release isentrope of overdrive plastic-bonded explosive PBX-9501
# R. S. Hixon, M. S. Shaw, J. N. Fritz, J. E. Vorthman, W. W. Anderson
# J. Appl. Phys. 88 (2000) pp 6287--6293
    V0      = 1/1.835
    rho_CJ  = 2.4403; P_CJ    = 35.3
    rho_min = 1.9;    rho_max = 3.2
    Pmin    = 18;     Pmax    = 80
    # fit to PCJ
    Gamma0  = 3
       nfCJ = 6
     fCJ(1) = 2.4287, 0.0378, 0.1005, -0.7166, 0.4537, 0.8062, -0.8473
    # fit to rho*Gamma
       ngCJ = 3
     gCJ(1) = 1.1520, -1.1490, 4.2382, -2.2234
}
