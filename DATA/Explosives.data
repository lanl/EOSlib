    FFQrate   = $EOSLIB_SHARED_LIBRARY_PATH/ExtHE
    # HErate    = $EOSLIB_SHARED_LIBRARY_PATH/ExtHE
    # Arrhenius = $EOSLIB_SHARED_LIBRARY_PATH/ExtHE
    # PnRate    = $EOSLIB_SHARED_LIBRARY_PATH/ExtHE
