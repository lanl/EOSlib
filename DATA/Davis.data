EOS:HEburn2=PBX9502.HotSpotCC.davis_SURFplus; units=hydro::std
{
    P_vac = 1.e-6
    OneDFunc_abs_tol = 1e-12
    OneDFunc_rel_tol = 1e-12
    ODE_epsilon      = 1e-12
    PTtol = 1.e-12
