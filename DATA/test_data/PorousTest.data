.include DataBaseUnits.data

:SharedLibs=EOS
{
    EOS               = NULL
    Hayes             = NULL
    EqPorous          = $EOSLIB_SHARED_LIBRARY_PATH/libPorous
    DissipativePorous = $EOSLIB_SHARED_LIBRARY_PATH/libPorous
    FrozenPorous      = $EOSLIB_SHARED_LIBRARY_PATH/libPorous
}

:SharedLibs=PhiEq
{
    PhiEq = $EOSLIB_SHARED_LIBRARY_PATH/libPorous
    Phi0  = $EOSLIB_SHARED_LIBRARY_PATH/libPorous
    Phi1  = $EOSLIB_SHARED_LIBRARY_PATH/libPorous
    EC    = $EOSLIB_SHARED_LIBRARY_PATH/libPorous
}

.base =EOS
.units=hydro::std
#
# porous EOS = solid EOS + compaction law
#
EqPorous=estane; units=hydro::std
{
    V_ref = 0.84
    EOS   = "Hayes::estane"
    PhiEq =  "Phi0::estane"
}

EqPorous=HMX.EC11fit; units=hydro::std
{
    EOS   = "Hayes::HMX"
    PhiEq = "Phi1::EC11.fit"
}

EqPorous=HMX.EC11; units=hydro::std
{
    EOS   = "Hayes::HMX"
    PhiEq = "EC::EC11"
}

FrozenPorous=HMX.65f90; units=hydro::std
{
    EOS   = "Hayes::HMX"
    PhiEq =  "Phi1::EC.65"
    phi = 0.9034
}
#
# solid EOS
#
Hayes=estane;
{
    rho0 = 1.2      # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    K0 = 7.37       # GPa
    N  = 5.8
    Cv = 1.e-3      # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
}

Hayes=HMX;
{
    rho0 = 1.9      # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    K0 = 12.9       # GPa
    K0 = 13.5       # Olingers hydrostatic data
    N  =  9.3
    Cv = 1.5e-3     # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    #c0=2.6778 s = 2.68946
}
#
# Compaction laws
#
PhiEq:Phi0=estane; units=hydro::std
{
    phi0    = 0.985         # initial porosity
    Pc      = 0.1           # crushup pressure (GPa)
    V0      = 0.833333      # estane V0 (cm^3/g)
    P0      = 1.0e-4        # HMX P0 (GPa)
    abs_tol=1e-4
    rel_tol=1e-4
    #phi0=0.95
    #abs_tol=1e-6
    #rel_tol=1e-6
}

PhiEq:Phi1=EC11.fit; units=hydro::std
{   # fit to Elban-Chiarito data for HMX11
    V0      = 0.526316      # HMX V0 (cm^3/g)
    P0      = 1.0e-4        # HMX P0 (GPa)
    PV0     = P0*V0
    PV1     = 0.7*V0
    phi0    = 0.64
    phi1    = 1
    #
    n = 10
    a = 1.6
    dphidP0 = 0.04
    dphidP1 = 0.2
    abs_tol = 1e-5
    rel_tol = 1e-5
    sphi0 = phi0
}

PhiEq:EC=EC11; units=hydro::std
{   # Elban-Chiarito data for HMX11
    expt = "HMX11"
    hmx  = "Hayes::HMX"
}

PhiEq:Phi1=EC.65; units=hydro::std
{
    :use=EC11.fit           # scale fit Elban-Chiarito data for HMX11
    sphi0   = 0.65          # scaled from 0.64
    sPV1    = 1             # scale factor of PV1
}

