    T0   = 300      # degrees K
    K0   =  14.7    # GPa
    Kp0  =   8.6
    Cv   = 1.5e-3   # (MJ/kg)/K
    G0 = G1 = 1.1   # Gamma/V = const
    e0 = T0*Cv
