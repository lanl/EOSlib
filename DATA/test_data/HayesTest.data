.include DataBaseUnits.data

:SharedLibs=EOS
{
    EOS   = NULL
    Hayes = $EOSLIB_DATA_PATH/../src/EOStypes/Hayes/libHayes
}

.base =EOS
.units=hydro::std


Hayes=HMX;
{
    rho0 = 1.9      # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    K0 = 12.9       # GPa
    K0 = 13.5       # Olingers hydrostatic data
    N  =  9.3
    Cv = 1.5e-3     # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    #c0=2.6778 s = 2.68946
}

Hayes=TPX;
{
    V0 = 1.20482    # cm^3/g
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    Cv = 1.05e-3    # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    K0 = 3.6        # GPa
    N  = 5.5
    #c0 = 2.17 s  = 1.52    # from Ric Gustavsen
}

Hayes=PMMA;
{
    rho0 = 1.186    # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    Cv = 1.05e-3    # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    c0 = 2.57       # sound speed, km/s
    s  = 1.54       # d(u_s)/d(u_p)
}

Hayes=Kel-F;
{
    rho0 = 2.122    # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    Cv = 1.05e-3    # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    c0 = 2.03       # sound speed, km/s
    s  = 1.64       # d(u_s)/d(u_p)
}

Hayes=estane;
{
    rho0 = 1.2      # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    K0 = 7.37       # GPa
    N  = 5.8
    Cv = 1.e-3      # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
}


