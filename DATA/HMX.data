HayesBM=HMX;  units=hydro::std
{
# Ref: "Fitting Forms for Isothermal Data"
#      Ralph Menikoff and Thomas D. Sewell
#      High Pressure Research 21 (2001) pp. 121-138
    rho0 = 1.9      # g/cm^3
    P0   = 1.0e-4   # GPa
    T0   = 300      # degrees K
    K0   =  14.7    # GPa
    Kp0  =   8.6
    Cv   = 1.5e-3   # (MJ/kg)/K
    G0 = G1 = 1.1   # Gamma/V = const
    e0 = T0*Cv
}
