utilize Amrita::Build

foreach src ( EOSdata, ZNDprofile, Detonation, Deflagration )
  echo Application/$src.C
  NewExec {
    src=$src
    libs=EOSapi,EOScc
  }
end foreach
foreach src ( ZNDprofileTW )
  echo Application/$src.C
  NewExec {
    src=$src
    libs=EOSapi,EOScc,ExtEOStypes,EOStypes
  }
end foreach
foreach src ( Wave, Isotherm, ImpedanceMatch )
  echo Application/$src.C
  NewExec {
    src=$src
    libs=EOSapi,EOScc,Porous
  }
end foreach

