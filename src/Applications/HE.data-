/*** Test TPX ***/
EOS:Keane=TPX; units=hydro::std
{
# ploymethylpentene, C6 H12
# ref
# Tariq, note dated July 14, 2010
# Multiple Shock Experiments in Lexan-Ta-TPX-LiF
    P_vac = -1              # reset to Pmin
    V_ref = 1/0.831
    e_ref = 0.22289
    P_ref = 1.e-4 
    T_ref = 300.
    V0  = 1/0.831           # cm^3/g (V0 at T=0)
    e0  = 0
    # 
    K0 =  2.7               # GPa
    Kp0 = 7.9               # dimensionless
    Kp0infty =  5.2         # dimensionless
    # corrected for cold curve
    V_PTref = V0
    P0_ref = P_ref
    # Gamma = a + b*(V/V0)
    a   =  0.57             # dimensionless
    b   =  0.0              # dimensionless
    eps1 = 0.3
    # Specific heat = Cv*sT^3/(cv_1+sT*cv_2+sT^2*cv_3+sT^3*cv_4) # MJ/kg/K
    # where sT = T/theta(V), and theta(V0) = 1 K
    # generic parameters
    # classical limit at Tlimit=3000 K
    # 1/2 classical limit at 300 K
    AtWt_H =  1.079                 # Atomic Weight H,  g/mol
    AtWt_C = 12.011                 # Atomic Weight C,  g/mol
    MolWt = 6*AtWt_C + 12*AtWt_H    # molecular weight, g/mol
    atoms = 6+12                    # atoms/molecule
    R  = 8.314472                   # Gas constant,    (J/mol)/K
    Cv = 3*atoms*R/(1000*MolWt)     # classical limit, (MJ/kg)/K = (J/mg)/K
    Tlimit = 3000                   # Tclassical/theta(V0)
    cv(1) =  1e-4 * Tlimit^3
    cv(2) =  0.01 * Tlimit^2
    cv(3) = -0.01 * Tlimit
    cv(4) =  1
}
EOS:Keane=Lexan; units=hydro::std
{
# polycarbonate, C16 H14 O3
# ref
# Tariq, note dated July 14, 2010
# Multiple Shock Experiments in Lexan-Ta-TPX-LiF
    P_vac = -1              # reset to Pmin
    V_ref = 1/1.193
    e_ref = 0.22289
    P_ref = 1.e-4 
    T_ref = 300.
    V0  = 1/1.193           # cm^3/g (V0 at T=0)
    e0  = 0
    # 
    K0 =   4.44             # GPa
    Kp0 = 11.               # dimensionless
    Kp0infty =  4.1         # dimensionless
    # corrected for cold curve
    V_PTref = V0
    P0_ref = P_ref
    # Gamma = a + b*(V/V0)
    a   =  0.0              # dimensionless
    b   =  0.6              # dimensionless
    eps1 = 0.3
    # Specific heat = Cv*sT^3/(cv_1+sT*cv_2+sT^2*cv_3+sT^3*cv_4) # MJ/kg/K
    # where sT = T/theta(V), and theta(V0) = 1 K
    # generic parameters
    # classical limit at Tlimit=3000 K
    # 1/2 classical limit at 300 K
    AtWt_H =  1.079                 # Atomic Weight H,  g/mol
    AtWt_C = 12.011                 # Atomic Weight C,  g/mol
    AtWt_O = 15.994                 # Atomic Weight O,  g/mol
    MolWt = 16*AtWt_C + 14*AtWt_H +3*AtWt_O   # molecular weight, g/mol
    atoms = 16+14+3                 # atoms/molecule
    R  = 8.314472                   # Gas constant,    (J/mol)/K
    Cv = 3*atoms*R/(1000*MolWt)     # classical limit, (MJ/kg)/K = (J/mg)/K
    Tlimit = 3000                   # Tclassical/theta(V0)
    cv(1) =  1e-4 * Tlimit^3
    cv(2) =  0.01 * Tlimit^2
    cv(3) = -0.01 * Tlimit
    cv(4) =  1
}
