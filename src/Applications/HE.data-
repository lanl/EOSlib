    HErate    = "HotSpotRate2::PBX9502"
}

HErate:HotSpotRate2=PBX9502; units=hydro::std
{
# Sam's temperature dependent hot-spot rate model
#   lambda(s) = 1 - exp(-s^2)
#   (d/dt)s = dsdt(0) * D/D(Ps)
#
    # D/D(Ps) = (p/Ps)^n
    n =  0.
    # Ths = Ts + a*Ps
    a = 30
    # dsdt(0) = b * [erfc( (Ti-Ths)/sigma )]^(1/3)
    b = (1.2075e5)^(1/3)
    sigma =  345
    Ti    = 1900
    source_type = 3             # use rate constraint on time step
}

