EOS:UsUp=Nitromethane; units=hydro::HiP
{
# Ref: LASL Hugoniot Data
#      Marsh
#      p. 599
    P_vac = 1.e-8
    rho0 = 1.125            # g/cm^3
    P0 = 1.0e-6             # Mb
    T0 = 300                # degrees K
    Cv = 0.29/23.89e3       # cal/g/K -> (Mb*cc/g)/K
    G0 = G1 = 1.2           # Gamma = G0 + G1*(V/V0-1)
    c0= 0.165
    s = 1.64
    #
    #G0 = 0.6805; G1 =0     # Gamma = G0, Mader's value    
    Cv = 0.414/23.89e3      # cal/g/K -> (Mb*cc/g)/K (Mader's value)
                            # Cv at T ~ 1400 K
    # MolWt = 61            # molecular weight, g/mole
    # R = 8.314472          # gas constant, J / mol K
    # Cv = 3*7*R/MolWt      # classical limit, 2.86 J/(g*K) = 0.68 cal/(g*K)
}

EOS:BKW=Nitromethane; units=hydro::HiP
{
# Ref: Numerical Modeling of Detonation (first ed)
#      Mader
#      Table 1.1, p6
    P_vac = 1.e-8
    V0 = 1/1.125
    e0 = 0
    de = 0.1
    Pn(1)=-3.11585072896e0,-2.35968123302e0,  2.10663268988e-1, 3.80357006508e-3,-3.53454737231e-3
    en(1)=-1.39936678316e0, 4.79350272379e-1, 6.06707773429e-2, 4.10672785525e-3, 1.13326560531e-4
    Tn(1)= 7.79645302519e0,-5.33007196907e-1, 7.09019736856e-2, 2.06149976021e-2,-5.66139653675e-3
    Cv = 0.556/23.89e3        # cal/g/K -> (Mb*cc/g)/K
}
