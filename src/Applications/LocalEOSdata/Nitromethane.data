HErate:Arrhenius!=Nitromethane; units=hydro::std
{
# Ref: An Investigation of the Shock Initiation of Liquid Nitromethane
#      D. R. Hardesty
#      Combustion and Flame 27 (1976) pp. 229-251
    tol = 1.e-6
    n   =  1                # reaction order
    T_a =  23e3/1.987       # Ea(cal/mole) / R = 11.5e3 K
    k   =  2.6e3            # per microsec
    T_cutoff = 400
    #
    #T_a = 18e3
    #k   =  2.6e5
    #
    #k *= 10
    #k *= 2
# Nunziato & Kipp, fast rate
#T_a = 29700
#k   = 1e8
#n=2
}
HErate:Arrhenius!=Nitromethane.2; units=hydro::std
{
    :use=Nitromethane
    n=2                 # second order
    k*=10
    #
    T_a =  15.e3        # activation temperature
    k   =  1.5e5        # per microsec
