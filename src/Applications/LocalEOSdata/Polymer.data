#
# Polymers
#
/*****************
EOS:Hayes=Teflon; units=hydro::std
{
#Ref: Marsh polytertafluroethylene p467
    V0 = 1/2.152            # cm^3/g
    P0 = 1.0e-4             # GPa
    T0 = 300                # degrees K
    Cv = 1.05e-3            # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    c0 = 1.9
    s  = 1.62
}
****************/

EOS:Hayes=Kapton; units=hydro::std
{
#Ref: Marsh polyimide p 444
    V0 = 1/1.414            # cm^3/g
    P0 = 1.0e-4             # GPa
    T0 = 300                # degrees K
    Cv = 1.05e-3            # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    c0 = 2.5
    s  = 1.4
# Note: decomposes ~ 20 GPa
}

EOS:Hayes=epoxy; units=hydro::std
{
#Ref: Marsh Epon 828, p 417
#Note       decompositon up ~ 3 and Ps ~ 27
    V0 = 1/1.185            # cm^3/g
    P0 = 1.0e-4             # GPa
    T0 = 300                # degrees K
    Cv = 1.05e-3            # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    c0 = 2.6
    s  = (9.74-c0)/5.1
}
