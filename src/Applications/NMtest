./ZNDprofile file HE.data material HEburn::Nitromethane.sam.de.2 \
   printddt nsteps 100

pl/ZNDprofile.pl out=Plots/NM2-u file=HE.data  \
   HE1=HEburn::Nitromethane.sam.de.2 HE2=HEburn::Nitromethane.sam.de.2T3000 \
   xlimits=-1,0.1 yvar=u

pl/ZNDprofile.pl out=Plots/NM2-rate file=HE.data \
   HE1=HEburn::Nitromethane.sam.de.2 HE2=HEburn::Nitromethane.sam.de.2T3000 \
   xvar=lambda yvar=rate

pl/ZNDprofile.pl out=Plots/NM2-P file=HE.data \
   HE=HEburn::Nitromethane.sam.de.2  \
   xvar=lambda yvar=P

pl/ZNDprofile.pl out=Plots/NM2-U file=HE.data \
   HE=HEburn::Nitromethane.sam.de.2  \
   xvar=lambda yvar=u
#
pl/ZNDprofile.pl out=Plots/NM2cutoff-xiu file=HE.data  \
   HE1=HEburn::Nitromethane.sam.de.2 HE2=HEburn::Nitromethane.sam.de.2T3000 \
   xlimits=-1,0.1 yvar=u nsteps=500

pl/ZNDprofile.pl out=Plots/NM2cutoff-xiLambda file=HE.data  \
   HE1=HEburn::Nitromethane.sam.de.2 HE2=HEburn::Nitromethane.sam.de.2T3000 \
   xlimits=-1,0.1 yvar=lambda nsteps=500
#
pl/ZNDprofile.pl out=Plots/NM2-xiu file=HE.data  \
   HE1=HEburn::Nitromethane.sam.de HE2=HEburn::Nitromethane.sam.de.2 \
   xlimits=-1,0.1 yvar=u nsteps=500

pl/ZNDprofile.pl out=Plots/NM2-xiLambda file=HE.data  \
   HE1=HEburn::Nitromethane.sam.de HE2=HEburn::Nitromethane.sam.de.2 \
   xlimits=-1,0.1 yvar=lambda nsteps=500

