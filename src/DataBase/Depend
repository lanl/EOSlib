	SharedLib DataBase.so DataBase.a

shared_obj.o: shared_obj.C

shared_obj.so: shared_obj.o
	g++ -shared -Wl,--whole-archive,--export-dynamic -o shared_obj.so shared_obj.o \
           -Wl,--no-whole-archive $(LFLAGS) -lLocalMath
