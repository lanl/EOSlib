DataBase.a: Subs

Data:       DataBase.a -lCalc -lList -lLocalIo -lLocalMath -lString -ldl
Convert:    DataBase.a -lCalc -lList -lLocalIo -lLocalMath -lString -ldl
TestDB:     ./DataBase.so -lCalc -lList -lLocalIo -lLocalMath -lString -ldl
Testdl:     -lLocalIo -ldl

ALL=SubMake LIBS $(EXECS) shared_obj.so


