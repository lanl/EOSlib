eosbase.a:  SubsBase
eosVe.a:    SubsVe
eosVT.a:    SubsVT
eos.a:      eosVe.a eosVT.a eosbase.a

#

Test1 : eos.a -lDataBase -lCalc     -lODE     -lOneDFunction \
              -lList     -lString   -lLocalIo -lLocalMath    -ldl
Test2 : ./EOS.so -ldl
Test3 : ./EOS.so -ldl

