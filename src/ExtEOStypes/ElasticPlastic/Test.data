.include ../../DataBase/Units.data

:SharedLibs=EOS_VT
{
    EOS_VT     = NULL
    Hayes      = NULL
    Shear1     = ./ElasticPlastic.so
}

:SharedLibs=EOS
{
    EOS        = NULL
    Hayes      = NULL
    VonMises   = ./ElasticPlastic.so
    ElasticEOS = ./ElasticPlastic.so
}
#
.base =EOS_VT
.units=hydro::std
#
# solid EOS
#
Hayes=HMX;
{
    T_ref = 300
    rho0 = 1.9      # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    K0 = 12.9       # GPa
    K0 = 13.5       # Olingers hydrostatic data
    N  =  9.3
    Cv = 1.5e-3     # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    #c0=2.6778 s = 2.68946
}

EOS:Hayes=HMX;
{
    rho0 = 1.9      # g/cm^3
    P0 = 1.0e-4     # GPa
    T0 = 300        # degrees K
    K0 = 12.9       # GPa
    K0 = 13.5       # Olingers hydrostatic data
    N  =  9.3
    Cv = 1.5e-3     # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    #c0=2.6778 s = 2.68946
}

#

Shear1=HMX
{
    EOS_VT = "Hayes::HMX"
    G      = 12.
}

EOS:ElasticEOS=HMX
{
    Elastic_VT = "Shear1::HMX"
}

EOS:VonMises=HMX
{
    Elastic = "Shear1::HMX"
     Y = 0.26
    nu = 0.11
}

EOS:VonMises=HMX1
{
    Elastic = "Shear1::HMX"
     Y = 0.26
    nu = 0.11
    #
    A1 = 8; n = 2
    A2 = 1.6
    tau = 0.1
}


:SharedLibs.=EOS_VT
{
    BirchMurnaghan = $SharedLibDirEOS/MaterialLib.so
}
EOS_VT:BirchMurnaghan=HMX; units=hydro::std
{
    T_ref = 300.
    V_ref = 0.52146445  # P_ref = 1.e-4
    V0  = 0.511509      # cm^3/g
    e0  = 0
    K0  = 17.66         # GPa
    Kp0 =  8.5          # dimensionless
    # Gamma = a + b*(V/V0)
    b   =  -0.25        # dimensionless
    a   =  1.20         # dimensionless
    # Cv = T^3/(cv_1+T*cv_2+T^2*cv_3+T^3*cv_4) # MJ/kg/K
    cv(1) = 5.2653675003188e-1, 3.0733581008896e2    
    cv(3) = 1.8318931090834e5,  4.1941409666532e2    
}

EOS_VT:Shear1=HMX.BM; units=hydro::std
{
    EOS_VT = "BirchMurnaghan::HMX"
    #G = 12.
    G = 11.
}

EOS:VonMises=HMX.BM; units=hydro::std
{
    Elastic = "Shear1::HMX.BM"
     Y = 0.26
    nu = 1.8
    A1 = 8; n = 2
    #
    #Y  = 0.26
    Y  = 0.25
    nu = 3
    A1 = 12; n = 1    
}

