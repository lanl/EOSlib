.include ../../DataBase/Units.data

:SharedLibs=EOS
{
    EOS                    = NULL
    Hayes                  = NULL
    VonMisesElasticPlastic = ./Uniaxial.so
}

.base =EOS
.units=hydro::std

#

Hayes=HMX; units=hydro::std
{
    rho0 = 1.9              # g/cm^3
    P0 = 1.0e-4             # GPa
    T0 = 300                # degrees K
    K0 = 13.5               # GPa
    N  =  9.3
    Cv = 1.5e-3             # (MJ/kg)/K
    Gamma0 = 1.1
    e0 = T0*Cv
    #c0=2.6778 s = 2.68946
}

Hayes=Al2024; units=hydro::std
{
# Ref: G. R. Fowles
#      Shock Wave Compression of Hardened and Annealed 2024 Aluminium
#      J. Appl. Phys. 32 (1961) pp. 1475-1487
# LASL shock Hugoniot Data p 166
    rho0 = 2.794            # g/cm^3
    P0 = 1.0e-4             # GPa
    T0 = 300                # degrees K
    Cv = 9.25e-4            # (MJ/kg)/K
    Gamma0 = 2.13
    e0 = T0*Cv
    c0 = 5.37
     s = 1.17
}

#

VonMisesElasticPlastic=HMX; units=hydro::std
{
    eos = "Hayes::HMX"
      G = 10                # GPa
      Y = 0.3               # GPa
}

VonMisesElasticPlastic=Al2024hard; units=hydro::std
{
# Ref: G. R. Fowles
#      Shock Wave Compression of Hardened and Annealed 2024 Aluminium
#      J. Appl. Phys. 32 (1961) pp. 1475-1487
# LASL shock Hugoniot Data p 166
    eos = "Hayes::Al2024"
      G = 25                # GPa
      Y =  0.24             # GPa
}
