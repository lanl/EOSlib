Test3:	    $(CC_LIB)/EOS.so -ldl ./GenHayes.so

Isobar:	    $(CC_LIB)/EOS.so $(CWD)/EOStypes/GenHayes/GenHayes.so -ldl
HotSpot:	$(CC_LIB)/EOS.so $(CWD)/EOStypes/GenHayes/GenHayes.so -ldl
#
MinP:       -lLocalIo  -lLocalMath -lm
#

GenHayesVe.a:	SubsVe
