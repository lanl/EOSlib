OS=_OS
CC_INCLUDE=$(CWD)/include
CC_LIB=$(CWD)/lib/$(OS)

IFLAGS=-I. -I$(CC_INCLUDE) 
IFLAGS_SUB=-I.. -I$(CC_INCLUDE)

INCLUDE_DIR=$(CC_INCLUDE)
EXEC_DIR=$(CWD)/bin/$(OS)
LIB_DIR=$(CWD)/lib/$(OS)

#

IdealGas:	IdealGas.a -leos  -lDataBase -lODE -lOneDFunction -lList \
                       -lCalc -lLocalIo  -lLocalMath -lString -ldl
Test1:	    IdealGas.a -leos  -lDataBase -lODE -lOneDFunction -lList \
                   -lCalc -lLocalIo  -lLocalMath -lString -ldl

Test2:	    $(CC_LIB)/EOS.so -ldl ./IdealGas.so
Test2VT:	$(CC_LIB)/EOS.so -ldl ./IdealGas.so

TestDetonation:	   IdealGas.a -leos  -lDataBase -lODE -lOneDFunction -lList \
                   -lCalc -lLocalIo  -lLocalMath -lString -ldl

TestDeflagration:  IdealGas.a -leos  -lDataBase -lODE -lOneDFunction -lList \
                   -lCalc -lLocalIo  -lLocalMath -lString -ldl

TestPolar:  IdealGas.a -leos  -lDataBase -lODE -lOneDFunction -lList \
                   -lCalc -lLocalIo  -lLocalMath -lString -ldl

#

IdealGasVe.a:	SubsVe
IdealGasVT.a:	SubsVT
IdealGas.a:	    IdealGasVe.a IdealGasVT.a


#

.INSTALL
IdealGas.h            IdealGas_VT.h            
IdealGas_Hugoniot.h   IdealGas_Isentrope.h    IdealGas_Isotherm.h
IdealGas_Detonation.h IdealGas_Deflagration.h
IdealGas_Riemann.h
IdealGas.a

#

.INCLUDE

IdealGas.so: IdealGas.a
	SharedLib IdealGas.so IdealGas.a
