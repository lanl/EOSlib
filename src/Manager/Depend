Test:   ./EOSmanager.so $(LIB_DIR)/EOS.so -ldl
#

.INSTALL
EOSmanager.h  EOSstate.h  PointerArray.h  SplitWord.h
EOSmanager.a 

.INCLUDE

EOSmanager.so: EOSmanager.a
	SharedLib EOSmanager.so EOSmanager.a 

InstallEOSmanager: EOSmanager.so
	@cp EOSmanager.so $(LIB_DIR)/EOSmanager.so; \
	$(ECHO) "*** shared object $(LIB_DIR)/EOSmanager.so installed"
