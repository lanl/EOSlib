############################
$(LIB)(ErrorHandler.o) : ErrorHandler.C
	$(CC_COMPL) $(DEBUG) $(DFLAGS) $(CCFLAGS) $(IFLAGS) -c ErrorHandler.C;
	ar ru $(LIB) ErrorHandler.o;
	rm ErrorHandler.o
	

$(LIB)(IoManip.o) : IoManip.C
	$(CC_COMPL) $(DEBUG) $(DFLAGS) $(CCFLAGS) $(IFLAGS) -c IoManip.C;
	ar ru $(LIB) IoManip.o;
